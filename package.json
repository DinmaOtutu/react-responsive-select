{
  "name": "react-responsive-select",
  "version": "4.5.3",
  "description": "A React customisable, touchable, single-select / multi-select form component. Built with keyboard and screen reader accessibility in mind.",
  "main": "dist/ReactResponsiveSelect.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.demo.js --port 3001 & yarn prettier-watch",
    "build": "./auto/build.sh",
    "deploy": "./auto/deploy.sh",
    "eslint": "eslint src/**",
    "test": "yarn eslint && jest",
    "coverage": "rm -rf coverage && jest --coverage && open $(pwd)/coverage/index.html",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "testURL": "http://localhost/",
    "coverageReporters": [
      "html"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ]
  },
  "author": "Ben Bowes <bb@benbowes.com>",
  "homepage": "https://benbowes.github.io/react-responsive-select/demo/",
  "repository": {
    "type": "git",
    "url": "https://github.com/benbowes/react-responsive-select"
  },
  "keywords": [
    "select",
    "react select",
    "react dropdown",
    "accessible react select",
    "component",
    "responsive",
    "mobile",
    "touch",
    "select",
    "dropdown",
    "accessible",
    "multiselect"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.1",
    "singleline": "1.0.x"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "@types/jest": "^23.3.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.6.0",
    "onchange": "^4.0.0",
    "prettier": "^1.14.2",
    "react": "^16.3.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.3.2",
    "sinon": "^4.5.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  },
  "peerDependencies": {
    "react": "0.14.9 || >15.3.0"
  }
}
